	<xs:complexType name="Country">
		<xs:annotation><xs:documentation xml:lang="en">
			Country name in address element.
			A country element must include an unambiguous country ISO 3166 2 letter code.
		</xs:documentation></xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="code" type="golfml:ISO3166CountryCodeEnum" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>


	<xs:simpleType name="ContactTypeEnum">
		<xs:annotation><xs:documentation xml:lang="en">
			List of values for contact type information.
		</xs:documentation></xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="club-house"/>
			<xs:enumeration value="reception"/>
			<xs:enumeration value="restaurant"/>
			<xs:enumeration value="reservation"/>
			<xs:enumeration value="management"/>
			<xs:enumeration value="technical"/>
			<xs:enumeration value="corporate"/>
			<xs:enumeration value="home"/>
			<xs:enumeration value="work"/>
			<xs:enumeration value="mobile"/>
			<xs:enumeration value="other"/>
		</xs:restriction>
	</xs:simpleType>


	<xs:simpleType name="MediaTypeEnum">
		<xs:annotation><xs:documentation xml:lang="en">
			List of values for types of media used in golfml.
		</xs:documentation></xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="photograph"/>
			<xs:enumeration value="video"/>
			<xs:enumeration value="hole-fly-over"/>
			<xs:enumeration value="hole-presentation"/>
			<xs:enumeration value="hole-yardage"/>
			<xs:enumeration value="yardage-book"/>
			<xs:enumeration value="course-map"/>
			<xs:enumeration value="google-earth"/>
			<xs:enumeration value="other"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:attributeGroup name="MediaTypeAttributes">
		<xs:annotation>
			<xs:documentation xml:lang="en">
				Group of common attributes used to characterise a media associated with either a country-club, a golf-course,
				or a hole, anywhere a media is accepted.
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="type" type="golfml:MediaTypeEnum">
			<xs:annotation><xs:documentation xml:lang="en">
				Type of information conveyed by the media: Photograph, video, yardage book, scan of scorecard, scan of golf card, map...
			</xs:documentation></xs:annotation>
		</xs:attribute>
		<xs:attribute name="content-type" type="xs:string"> <!-- Should be limited to valid content-types? -->
			<xs:annotation><xs:documentation xml:lang="en">
				Technical type of the media (MIME (Multipurpose Internet Mail Extensions) type of the media.
				Example: video/mpeg, text/xml...
			</xs:documentation></xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="golfml:Other"/>
	</xs:attributeGroup>
	
	<xs:group name="MediaChoice">
		<xs:annotation><xs:documentation xml:lang="en">
			A media is either embedded in the golfml document or a URI (link) to it.
			We currently removed the ability to store media inside a golfml document for performance purposes
			and to limit the size of golfml document.
			Embedded storage of media in golfml document should be limited to small size items such as vector logo
			and small images.
		</xs:documentation></xs:annotation>
		<xs:choice>
			<xs:element name="url" type="xs:anyURI">
				<xs:annotation><xs:documentation xml:lang="en">
					Location of the media.
				</xs:documentation></xs:annotation>
			</xs:element>
			<xs:element name="content" type="xs:base64Binary">
				<xs:annotation><xs:documentation xml:lang="en">
					<!-- PROBLEM: Shall we accept image content in golfml file? File could be quite large and clumpsy to process... -->
					Base64-coded content of the media. Use of content embedded in golfml document is discouraged and should be limited
					to small media files (less than 2000 bytes).
				</xs:documentation></xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:group>
	   
	<xs:complexType name="Media">
		<xs:annotation><xs:documentation xml:lang="en">
			A media is a piece of multi-media information attached to either a country-club, a golf-course, a hole, a player,
			or a round of golf. Example of such media are:
			* country-club: Picture of club house, access map.
			* golf-course: scenic pictures of holes, yardage book...
			* hole: yardage page, pin placement chart...
			* player: picture, scan of official golf card...
			* round of golf: souvenir of a memorable golfing moment...
		</xs:documentation></xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="xs:string">
				<xs:annotation><xs:documentation xml:lang="en">
					Name of the media.
				</xs:documentation></xs:annotation>
			</xs:element>
			<xs:element name="description" type="xs:string" minOccurs="0">
				<xs:annotation><xs:documentation xml:lang="en">
					Description of the media.
				</xs:documentation></xs:annotation>
			</xs:element>
			<xs:group ref="golfml:MediaChoice"/>
		</xs:sequence>
		<xs:attributeGroup ref="golfml:MediaTypeAttributes"/>
	</xs:complexType>
	
	
	<xs:simpleType name="Rating">
		<xs:annotation><xs:documentation xml:lang="en">
			The rating type defines a standard, generic, evaluation criteria for golfml entities.
			Rating is limited to 0 to 5 "stars". Decimal faction allowed.
		</xs:documentation></xs:annotation>
		<xs:restriction base="xs:float">
			<xs:maxInclusive value="5"/>
			<xs:pattern value="[0-5]{0,1}(\.[0-9]{1,1})?"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="LocalisedText">
		<xs:annotation><xs:documentation xml:lang="en">
			Simple type to add language information on comments and notes.
		</xs:documentation></xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="lang" type="xs:language" default="en"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="Note">
		<xs:annotation><xs:documentation xml:lang="en">
			Generic type for addition of comments and/or rating.
			The text of the note is localised to a xml language.
		</xs:documentation></xs:annotation>
		<xs:sequence>
			<xs:element name="comment" type="golfml:LocalisedText" maxOccurs="unbounded">
				<xs:annotation><xs:documentation xml:lang="en">
					Localised, textual comment about the parent entitiy.
				</xs:documentation></xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="rating" type="golfml:Rating">
			<xs:annotation><xs:documentation xml:lang="en">
				Simple rating of the parent entitiy. Always optional.
			</xs:documentation></xs:annotation>
		</xs:attribute>
		<xs:attribute name="date" type="xs:dateTime" use="optional">
			<xs:annotation><xs:documentation xml:lang="en">
				Date of issue of the comment.
			</xs:documentation></xs:annotation>
		</xs:attribute>
	</xs:complexType>
	
	
	<!-- Geographical types (Copied from KML 2.1) -->
	
	<xs:simpleType name="Anglepos90">
		<xs:annotation><xs:documentation xml:lang="en">
				Angle between 0 and 90 degrees.
		</xs:documentation></xs:annotation>
		<xs:restriction base="xs:double">
			<xs:minInclusive value="0.0"/>
			<xs:maxInclusive value="90.0"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="Angle90">
		<xs:annotation><xs:documentation xml:lang="en">
				Angle between -90 and 90 degrees.
		</xs:documentation></xs:annotation>
		<xs:restriction base="xs:double">
			<xs:minInclusive value="-90"/>
			<xs:maxInclusive value="90.0"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="Angle180">
		<xs:annotation><xs:documentation xml:lang="en">
				Angle between -180 and 180 degrees.
		</xs:documentation></xs:annotation>
		<xs:restriction base="xs:double">
			<xs:minInclusive value="-180"/>
			<xs:maxInclusive value="180"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="Angle360">
		<xs:annotation><xs:documentation xml:lang="en">
				Angle between -360 and 360 degrees.
		</xs:documentation></xs:annotation>
		<xs:restriction base="xs:double">
			<xs:minInclusive value="-360"/>
			<xs:maxInclusive value="360"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="AltitudeModeEnum">
		<xs:annotation><xs:documentation xml:lang="en">
				KML/Google Earth "altitude mode".
				See also: http://code.google.com/intl/en/apis/kml/documentation/altitudemode.html.
		</xs:documentation></xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="relativeToSeaFloor"/>
			<xs:enumeration value="clampToSeaFloor"/>
			<xs:enumeration value="relativeToGround"/>
			<xs:enumeration value="clampToGround"/>
			<xs:enumeration value="absolute"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:complexType name="Altitude">
		<xs:annotation><xs:documentation xml:lang="en">
				Altitude of a point. Use mode attribute to determine semantics.
		</xs:documentation></xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:double">
				<xs:attribute name="mode" type="golfml:AltitudeModeEnum" use="optional" default="absolute"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	
	
	<!-- GPS and position related types -->
	
	<xs:complexType name="PositionGPS">
		<xs:annotation><xs:documentation xml:lang="en">
			GPS coordinate of a point.
		</xs:documentation></xs:annotation>
		<xs:all>
			<xs:element name="lat" type="golfml:Angle90">
				<xs:annotation><xs:documentation xml:lang="en">
					Latitude of the GPS position expressed as a float value between -90째 (South Pole) to +90째 (North Pole).
				</xs:documentation></xs:annotation>
			</xs:element>
			<xs:element name="lon" type="golfml:Angle180">
				<xs:annotation><xs:documentation xml:lang="en">
					Longitude in the GPS position expressed as a float value between -180째 for longitude West of Greenwich
					to +180째 for longitude East of Greenwich.
				</xs:documentation></xs:annotation>
			</xs:element>
			<xs:element name="alt" type="golfml:Altitude" minOccurs="0">
				<xs:annotation><xs:documentation xml:lang="en">
					Altitude of a GPS position expressed as a float value. Meaning and units of the value depends on the
					mandatory 'mode' attribute.
				</xs:documentation></xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	
	<xs:complexType name="Position">
		<xs:annotation><xs:documentation xml:lang="en">
			Coordinate of a point. The coordinate of a point can be either a GPS coordinate or
			coordinates in any specific system.
		</xs:documentation></xs:annotation>
		<xs:choice>
			<xs:element name="position-gps" type="golfml:PositionGPS">
				<xs:annotation><xs:documentation xml:lang="en">
					GPS coordinate of a point.
				</xs:documentation></xs:annotation>
			</xs:element>
			<xs:element name="position-other" type="golfml:RoomForExtension">
				<xs:annotation><xs:documentation xml:lang="en">
					Coordinate of a point in an arbitraty unit system.
				</xs:documentation></xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	
	<xs:complexType name="PositionWithSequence">
		<xs:annotation><xs:documentation xml:lang="en">
			Coordinate of a point in a list of points (polygon).
		</xs:documentation></xs:annotation>
		<xs:complexContent>
			<xs:extension base="golfml:Position">
				<xs:attribute name="number" type="xs:nonNegativeInteger" use="required"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:simpleType name="OrientationEnum">
		<xs:annotation><xs:documentation xml:lang="en">
			List of values to further document a point of interest on a golf course.
			Example: Point of interest is of type "green" and orientation of the point is "front"
					 to specify the front or start of the green.
		</xs:documentation></xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="front"/>
			<xs:enumeration value="middle"/>
			<xs:enumeration value="back"/>
			<xs:enumeration value="left"/>
			<xs:enumeration value="center"/>
			<xs:enumeration value="right"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:complexType name="PointOfInterest">
		<xs:annotation><xs:documentation xml:lang="en">
			Point of interest on a golf course.
			A point of interest contains a name, the coordinate of the point, a specification of its type limited to a list
			of valid values, and an optional orientation in respect to the point.
		</xs:documentation></xs:annotation>
		<xs:all>
			<xs:element name="name" type="xs:string" minOccurs="0">
				<xs:annotation><xs:documentation xml:lang="en">
					Name of point of interest.
				</xs:documentation></xs:annotation>
			</xs:element>
			<xs:element name="description" type="xs:string" minOccurs="0">
				<xs:annotation><xs:documentation xml:lang="en">
					Description of a point of interest.
				</xs:documentation></xs:annotation>
			</xs:element>
			<xs:element name="position" type="golfml:Position">
				<xs:annotation><xs:documentation xml:lang="en">
					Coordinate of a point of interest.
				</xs:documentation></xs:annotation>
			</xs:element>
		</xs:all>
		<xs:attribute name="number" type="xs:nonNegativeInteger"/>
		<xs:attribute name="type" type="golfml:GolfCourseAreaTypeEnum"/>
		<xs:attribute name="subtype" type="xs:string"/> <!-- green-side or fairway bunker, sand or grass bunker... -->
		<xs:attribute name="orientation" type="golfml:OrientationEnum"/>
		<xs:attributeGroup ref="golfml:Other"/>
	</xs:complexType>	
	
	<xs:complexType name="AreaOfInterest">
		<xs:annotation><xs:documentation xml:lang="en">
			Area of interest on a golf course.
			An area of interest contains a name, a list of coordinates of points, and a specification of its type
			limited to a list of valid values.
			
			Note: An AreaOfInterest can be a line (example: Out-of-bound line along a hole).
		</xs:documentation></xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="xs:string">
				<xs:annotation><xs:documentation xml:lang="en">
					Name of an area of interest.
				</xs:documentation></xs:annotation>
			</xs:element>
			<xs:element name="description" type="xs:string" minOccurs="0">
				<xs:annotation><xs:documentation xml:lang="en">
					Description of an area of interest.
				</xs:documentation></xs:annotation>
			</xs:element>
			<xs:element name="position" type="golfml:PositionWithSequence" minOccurs="2" maxOccurs="unbounded">
				<xs:annotation><xs:documentation xml:lang="en">
					Position of a point of the polygon that forms the area of interest.
					Positions are ordered with the 'number' attribute.
				</xs:documentation></xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="number" type="xs:nonNegativeInteger">
			<xs:annotation><xs:documentation xml:lang="en">
				Position of the area of interest in a list of areas of interest.
				This position can be used to "layer" areas of interest.
			</xs:documentation></xs:annotation>
		</xs:attribute>
		<xs:attribute name="type" type="golfml:GolfCourseAreaTypeEnum">
			<xs:annotation><xs:documentation xml:lang="en">
				Type of area of interest.
			</xs:documentation></xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="golfml:Other"/>
	</xs:complexType>
	
	<xs:complexType name="Placemarks">
		<xs:annotation><xs:documentation xml:lang="en">
			Placemarks is a collection of points and/or areas of interest.
		</xs:documentation></xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="poi" type="golfml:PointOfInterest">
				<xs:annotation><xs:documentation xml:lang="en">
					Point of interest.
				</xs:documentation></xs:annotation>
			</xs:element>
			<xs:element name="aoi" type="golfml:AreaOfInterest">
				<xs:annotation><xs:documentation xml:lang="en">
					Area of interest.
				</xs:documentation></xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	
	
